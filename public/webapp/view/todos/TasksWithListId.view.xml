<mvc:View
	controllerName="webapp.controller.todos.TasksWithListId"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">
	<Page id="TasksWithListId" title="{i18n>tasks}"
		class="sapUiResponsiveContentPadding">
		<content>
			<List id="TaskList" headerText="{i18n>TasksWithListId}" items="{viewState>/Task}">
				<items>
					<StandardListItem
						title="{viewState>/text}"/>
				</items>
			</List>
		</content>
	</Page>
</mvc:View>