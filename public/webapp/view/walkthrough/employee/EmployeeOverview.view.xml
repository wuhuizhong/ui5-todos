<mvc:View
	controllerName="webapp.controller.walkthrough.employee.EmployeeOverview"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:l="sap.ui.layout">
	<Page id="EmployeeOverviewParent" title="{i18n>EmployeeOverview}"
		showNavButton="true"
		navButtonPress="onNavBack"
		class="sapUiResponsiveContentPadding">
		<content>
			<!-- inserted by routing 
			在manifest.json指定Page id为 "EmployeeOverviewParent"，即target中controlId的内容。
			content将会被router用top 和 content 填充。
			-->
		</content>
	</Page>
</mvc:View>